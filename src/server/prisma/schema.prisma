enum ROLE {
  ADMIN
  USER
  SUPERADMIN
}

enum TRANSACTION_STATUS {
  PENDING
  PROCESSING
  SHIPPED
  IN_TRANSIT
  DELIVERED
  CANCELED
  RETURNED
  REFUNDED
}

enum PAYMENT_STATUS {
  PENDING
  PAID
  CANCELED
  FAILED
  REFUNDED
  PARTIALLY_REFUNDED
}

enum CART_STATUS {
  ACTIVE
  ABANDONED
  CONVERTED
}

enum CHAT_STATUS {
  OPEN
  RESOLVED
}

enum CART_EVENT {
  PENDING
  ADD
  CHECKOUT_STARTED
  CHECKOUT_COMPLETED
}

enum SECTION_TYPE {
  HERO
  PROMOTIONAL
  BENEFITS
  NEW_ARRIVALS
}
